{
  "name": "agenda-hibrida-tatuagem",
  "version": "2.0.0",
  "main": "server.js",
  "description": "Sistema híbrido de agenda visual para tatuadores com integração QNAP, Google Drive e Google Calendar",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:full": "concurrently \"npm run dev\" \"cd ../agenda-hibrida-frontend && npm run dev\"",
    "build": "echo 'Build do backend concluído'",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:all": "jest --coverage --testPathPattern=__tests__",
    "backup": "node scripts/backup.js",
    "setup": "node scripts/setup.js",
    "sync": "node scripts/sync-calendar.js",
    "migrate-folders": "node scripts/migrate-client-folders.js",
    "seed:mock": "node database/seed-mock-data.js",
    "seed:clear": "node database/seed-mock-data.js --clear",
    "migrate:audit": "sqlite3 agenda_hibrida.db < database/migrations/029_create_audit_logs.sql",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cleanup": "node scripts/cleanup-code.js",
    "depcheck": "depcheck",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "check": "npm run lint && npm run depcheck"
  },
  "keywords": [
    "agenda",
    "tatuagem",
    "híbrido",
    "qnap",
    "google-drive",
    "google-calendar"
  ],
  "author": "Manus AI",
  "license": "MIT",
  "dependencies": {
    "ag-psd": "^28.4.1",
    "archiver": "^7.0.1",
    "axios": "^1.12.2",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "canvas": "^3.2.0",
    "chokidar": "^3.5.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "google-auth-library": "^10.4.1",
    "googleapis": "^164.0.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.11.14",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-ical": "^0.19.0",
    "nodemailer": "^7.0.9",
    "path": "^0.12.7",
    "qrcode": "^1.5.4",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "webdav": "^5.7.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/jest": "^30.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "glob": "^11.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "wait-on": "^9.0.1"
  }
}
